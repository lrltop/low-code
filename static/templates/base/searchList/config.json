			"js": "import MescrollCompMixin from '@/components/mescroll/mixins/mescroll-comp.js';\n// Tips:请严格按照Vue2.x语法进行编写\nexport default {\n  mixins: [MescrollCompMixin],\n  name: \"search.vue\",\n  data() {\n    return {\n      \"list\": [],\n      \"historyList\": [\"酸菜鱼\"],\n      \"hotList\": [\"麻婆豆腐\", \"糖醋排骨\"],\n      \"isHotListShow\": true\n    };\n  },\n  // 生命周期\n  async onLoad(e) {},\n  // 调用方法\n  methods: {\n    async hideHotList(e = {}) {\n      this.isHotListShow = false;\n    },\n    async search(e = {}) {\n      let keyword = e.value;\n      let searchKeyList = uni.getStorageSync('searchKeyList') || [];\n      if (searchKeyList.indexOf(keyword) > -1) {\n        searchKeyList.splice(searchKeyList.indexOf(keyword), 1);\n      }\n      searchKeyList.unshift(keyword);\n      uni.setStorageSync('searchKeyList', searchKeyList);\n    },\n    async clearHistoryList(e = {}) {\n      this.historyList = [];\n    }\n  }\n};"
