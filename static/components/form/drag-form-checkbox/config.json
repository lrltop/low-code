        {
            "label": "下划线",
            "description": "",
            "classify": "功能",
            "key": "borderBottom",
            "type": "Boolean",
            "default": false,
            "must": {
                "key": "placement",
                "value": ["column"]
            }
        }
	],
	"events": [
		{
			"label": "勾选发生变化时触发",
			"key": "change",
			"description": "参数为选中value值数组",
			"params": []
		}
	]
