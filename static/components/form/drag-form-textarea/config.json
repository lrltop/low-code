{
	"componentName": "drag-form-textarea",
	"name": "文本域",
	"description": "",
	"props": [
        {
        	"label": "双向绑定",
        	"description": "",
        	"key": "v-model",
        	"type": "String",
        	"default": ""
        },
		{
			"label": "标题",
            "classify": "功能",
			"description": "",
			"key": "labelText",
			"type": "String",
			"default": "文本域"
		},
		{
			"label": "标题宽度",
			"description": "",
            "classify": "样式",
			"key": "labelWidth",
			"type": "String",
			"inputType": "size",
			"default": "120rpx"
		},
		{
			"label": "标题文字大小",
			"description": "",
            "classify": "样式",
			"key": "labelFontSize",
			"type": "String",
			"inputType": "size",
			"default": "30rpx"
		},
		{
			"label": "输入框边框大小",
			"description": "",
            "classify": "样式",
			"key": "borderWidth",
			"type": "String",
			"inputType": "size",
			"default": "1px"
		},
		{
			"label": "输入框边框样式",
			"description": "",
            "classify": "样式",
			"key": "borderStyle",
			"type": "String",
            "inputType": "select",
            "inputProps": {
                "sectionMode": "borderStyle"
            },
			"default": "solid"
		},
		{
			"label": "输入框边框圆角大小",
			"description": "",
            "classify": "样式",
			"key": "borderRadius",
			"type": "String",
			"inputType": "size",
			"default": "5rpx"
		},
		{
			"label": "标题文字颜色",
			"description": "",
            "classify": "样式",
			"key": "labelColor",
			"type": "String",
			"inputType": "color",
			"default": "#333333"
		},
		{
			"label": "输入框边框颜色",
			"description": "",
            "classify": "样式",
			"key": "borderColor",
			"type": "String",
			"inputType": "color",
			"default": "#f5f5f5"
		},
		{
			"label": "为空时占位符",
			"description": "",
            "classify": "功能",
			"key": "placeholder",
			"type": "String",
			"default": "请输入"
		},
		{
			"label": "最大输入长度",
            "classify": "功能",
			"description": "设置为 -1 的时候不限制最大长度",
			"key": "maxlength",
			"type": "Number",
			"default": 140
		},
		{
			"label": "光标与键盘的距离",
			"description": "单位 px 。取 textarea 距离底部的距离和 cursor-spacing 指定的距离的最小值作为光标与键盘的距离, 仅支持App、微信小程序、百度小程序、字节跳动小程序、飞书小程序、QQ小程序、京东小程序",
			"key": "cursorSpacing",
            "classify": "功能",
			"type": "Number",
			"default": 0,
            "inputProps": {
                "suffixText": "px"
            }
		},
		{
			"label": "获取焦点时光标位置",
            "classify": "功能",
			"description": "仅支持微信小程序、App、H5、百度小程序、字节跳动小程序、飞书小程序、QQ小程序、京东小程序",
			"key": "cursor",
			"type": "Number",
			"default": ""
            
		},
		{
			"label": "光标起始位置",
            "classify": "功能",
			"description": "自动聚集时有效，需与selectionEnd搭配使用，需要开启获取焦点(focus)，仅支持微信小程序、App、H5、百度小程序、字节跳动小程序、飞书小程序、QQ小程序、京东小程序",
			"key": "selectionStart",
			"type": "Number",
			"default": -1,
            "must": {
                "key": "focus",
                "value": [true]
            }
		},
		{
			"label": "获取焦点",
			"description": "在 H5 平台能否聚焦以及软键盘是否跟随弹出，取决于当前浏览器本身的实现",
			"key": "focus",
            "classify": "功能",
			"type": "Boolean",
			"default": false
		},
		{
			"label": "禁用",
            "classify": "功能",
			"description": "",
			"key": "disabled",
			"type": "Boolean",
			"default": false
		},
		{
			"label": "显示长度计数",
			"description": "",
            "classify": "功能",
			"key": "count",
			"type": "Boolean",
			"default": true
		},
		{
			"label": "京东小程序自动聚焦拉起键盘",
			"description": "仅支持京东小程序",
			"key": "autoFocus",
            "classify": "功能",
			"type": "Boolean",
			"default": false
		},
		{
			"label": "自动增高",
			"description": "设置auto-height时，样式高度height不生效",
			"key": "autoHeight",
            "classify": "功能",
			"type": "Boolean",
			"default": false
		},
		{
			"label": "光标结束位置",
            "classify": "功能",
			"description": "自动聚集时有效，需与selectionStart搭配使用，需要开启获取焦点(focus)，仅支持微信小程序、App、H5、百度小程序、字节跳动小程序、飞书小程序、QQ小程序、京东小程序",
			"key": "selectionEnd",
			"type": "Number",
			"default": -1,
            "must": {
                "key": "focus",
                "value": [true]
            }
		},
		{
			"label": "键盘右下角按钮文字",
            "classify": "功能",
			"description": "仅在 type=\"text\" 时生效，仅支持微信小程序基础库2.13.0+、App-vue和H5(hbx2.9.9+，且要求设备webview内核Chrome81+、Safari13.7+)",
			"key": "confirmType",
			"type": "String",
			"default": "done",
            "inputType": "select",
            "inputProps": {
                "options": [
                    { "label": "完成", "value": "done", "description": ""},
                    { "label": "发送", "value": "send", "description": ""},
                    { "label": "搜索", "value": "search", "description": ""},
                    { "label": "下一个", "value": "send", "description": ""},
                    { "label": "前往", "value": "go", "description": ""}
                ]
            }
		},
		{
			"label": "点击键盘右下角按钮保持键盘不收起",
            "classify": "功能",
			"description": "仅支持App(hbx3.3.7+)、H5 (hbx3.3.7+)、微信小程序 (基础库 2.16.0+)、百度小程序 (基础库 3.130.1+)、快手小程序",
			"key": "confirmHold",
			"type": "Boolean",
			"default": false
		},
		{
			"label": "显示键盘上方带有”完成“按钮一栏",
            "classify": "功能",
			"description": "仅支持微信小程序、百度小程序、QQ小程序、京东小程序",
			"key": "showConfirmBar",
			"type": "Boolean",
			"default": true
		},
		{
			"label": "键盘弹起自动上推页面",
            "classify": "功能",
			"description": "仅支持App-Android（softinputMode 为 adjustResize 时无效）、微信小程序、百度小程序、QQ小程序、京东小程序",
			"key": "adjustPosition",
			"type": "Boolean",
			"default": true
		},
		{
			"label": "键盘收起自动失去焦点",
            "classify": "功能",
			"description": "仅支持App（hbx3.0.0+）",
			"key": "autoBlur",
			"type": "Boolean",
			"default": false
		}
	],
	"events": [
		{
			"label": "点击事件",
			"key": "click",
			"description": "",
			"params": []
		},
        {
        	"label": "输入时触发",
        	"key": "input",
        	"description": "快手小程序不支持",
        	"params": []
        },
        {
        	"label": "输入框行数变化时调用",
        	"key": "linechange",
        	"description": "字节跳动小程序、飞书小程序、快手小程序不支持,nvue ios暂不支持",
        	"params": []
        },
        {
        	"label": "输入框聚焦时触发",
        	"key": "focus",
        	"description": "仅微信小程序、京东小程序、App（HBuilderX 2.0+ nvue uni-app模式 (opens new window)） 、QQ小程序支持 height",
        	"params": []
        },
        {
        	"label": "输入框失去焦点时触发",
        	"key": "blur",
        	"description": "快手小程序不支持",
        	"params": []
        },
        {
        	"label": "点击完成按钮时触发",
        	"key": "confirm",
        	"description": "仅支持微信小程序、百度小程序、QQ小程序、京东小程序",
        	"params": []
        },
        {
        	"label": "键盘高度发生变化的时候触发此事件",
        	"key": "keyboardheightchange",
        	"description": "仅支持微信小程序基础库2.7.0+、App (hbx3.1.0+)",
        	"params": []
        }
	]
}