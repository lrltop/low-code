{
    "componentName": "drag-navigation-backtop",
    "name": "返回顶部",
    "description": "用于长页面，滑动一定距离后，出现返回顶部按钮，方便快速返回顶部的场景",
    "order": "5",
    "props": [{
            "label": "按钮形状",
            "description": "",
            "key": "mode",
            "type": "String",
            "default": "circle",
            "inputType": "select",
            "inputProps": {
                "options": [
                    { "label": "圆形", "value": "circle" },
                    { "label": "矩形", "value": "square" }
                ]
            }
        },
        {
            "label": "图标",
            "description": "",
            "key": "icon",
            "type": "String",
            "default": "arrow-upward",
            "inputType": "uviewIcon"
        },
        {
            "label": "按钮文字",
            "description": "返回顶部按钮的提示文字",
            "key": "text",
            "type": "String",
            "default": ""
        },
        {
            "label": "返回时间",
            "description": "返回顶部过程中的过渡时间，单位ms",
            "key": "duration",
            "type": "Number",
            "default": 100,
            "inputProps": {
                "suffixText": "ms"
            }
        },
        {
            "label": "按钮到屏幕底部的距离",
            "description": "返回按钮位置到屏幕底部的距离，单位rpx",
            "key": "bottom",
            "type": "Number",
            "default": 100,
            "inputProps": {
                "suffixText": "rpx"
            }
        },
        {
            "label": "按钮到屏幕右边的距离",
            "description": "返回按钮位置到屏幕右边的距离，单位rpx",
            "key": "right",
            "type": "Number",
            "default": 20,
            "inputProps": {
                "suffixText": "rpx"
            }
        },
        {
            "label": "按钮层级",
            "description": "返回顶部按钮的层级",
            "key": "zIndex",
            "type": "Number",
            "default": 20
        }
    ],
    "events": [

    ],
    "slot": {
        "default": "组件底部"
    }
}
