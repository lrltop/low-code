{
	"componentName": "drag-data-parse",
	"name": "富文本解析器",
	"description": "",
	"props": [
        {
        	"label": "富文本字符串",
        	"description": "",
        	"key": "content",
        	"type": "String",
            "inputType": "textarea",
        	"default": "<div style=\"color: red;\">富文本<div>"
        },
        {
        	"label": "标签默认样式",
        	"description": "标签对象样式，点击编辑数据查看演示数据",
        	"key": "tagStyle",
        	"type": "Object",
            "inputType": "jsonEditor",
        	"default": {
                "p": "color: red;font-size:32rpx",
                "span": "font-size: 30rpx"
            }
        },
        {
        	"label": "背景颜色",
        	"description": "只适用与APP-PLUS-NVUE平台",
        	"key": "bgColor",
        	"type": "String",
            "inputType": "color",
        	"default": ""
        },
        {
        	"label": "主域名",
        	"description": "设置后将给链接自动拼接上主域名或协议名",
        	"key": "domain",
        	"type": "String",
        	"default": ""
        },
        {
        	"label": "图片出错时的占位图",
        	"description": "",
        	"key": "errorImg",
        	"type": "String",
        	"default": "",
            "inputType": "image"
        },
        {
        	"label": "图片加载完成前的占位图",
        	"description": "",
        	"key": "loadingImg",
        	"type": "String",
        	"default": "",
            "inputType": "image"
        },
        {
        	"label": "外部链接被点击时自动复制",
        	"description": "",
        	"key": "copyLink",
        	"type": "Boolean",
            "inputType": "boolean",
        	"default": true
        },
        {
        	"label": "图片懒加载",
        	"description": "nvue不支持此属性",
        	"key": "lazyLoad",
        	"type": "Boolean",
        	"default": true,
            "inputType": "boolean"
        },
        {
        	"label": "播放视频时自动暂停其它视频",
        	"description": "",
        	"key": "pauseVideo",
        	"type": "Boolean",
        	"default": true,
            "inputType": "boolean"
        },
        {
        	"label": "图片点击预览",
        	"description": "",
        	"key": "previewImg",
        	"type": "Boolean",
        	"default": true,
            "inputType": "boolean"
        },
        {
        	"label": "自动给表格添加一个滚动层",
        	"description": "使表格可以单独横向滚动",
        	"key": "scrollTable",
        	"type": "Boolean",
        	"default": false,
            "inputType": "boolean"
        },
        {
        	"label": "长按复制内容",
        	"description": "",
        	"key": "selectable",
        	"type": "Boolean",
        	"default": false,
            "inputType": "boolean"
        },
        {
        	"label": "自动将 title 标签的内容设置到页面标题",
        	"description": "",
        	"key": "setTitle",
        	"type": "Boolean",
        	"default": true,
            "inputType": "boolean"
        },
        {
        	"label": "图片被长按时显示菜单",
        	"description": "小程序",
        	"key": "showImgMenu",
        	"type": "Boolean",
        	"default": true,
            "inputType": "boolean"
        },
        {
        	"label": "使用页面内锚点",
        	"description": "",
        	"key": "useAnchor",
        	"type": "Boolean",
        	"default": false,
            "inputType": "boolean"
        }
        
	],
	"events": [
		{
			"label": "dom 加载完成时触发，$event为空",
			"key": "load",
			"description": "",
			"params": []
		},
        {
        	"label": "渲染完成时触发，$event返回 boundingClientRect 的查询结果（包含宽高、位置等信息），所有图片（除懒加载）加载完成时才会触发，图片较大时可能 延时较长",
        	"key": "ready",
        	"description": "",
        	"params": []
        },
        {
        	"label": "出错时触发，$event返回一个 object，其中 source 是错误来源，errMsg 为错误信息，target 包含出错标签的具体信息",
        	"key": "error",
        	"description": "",
        	"params": []
        },
        {
        	"label": "图片被点击时触发，$event返回一个 object，其中 src 是图片链接，ignore 是一个函数，在事件中调用将不进行预览；可用于阻挡 onShow 的调用",
        	"key": "imgTap",
        	"description": "",
        	"params": []
        },
        {
        	"label": "在链接被点击时触发，$event返回一个 object，其中包含了被点击的 a 标签的所有属性，ignore 是一个函数，在事件中调用后将不自动跳转/复制；可在该事件中进行下载文档等进一步操作",
        	"key": "linkTap",
        	"description": "",
        	"params": []
        }
	]
}