{
    "componentName": "drag-base-image",
    "name": "图片",
    "description": "图片",
    "order": "3",
    "iStyle": {
        "display": "inline-block"
    },
    "props": [{
            "label": "图片链接",
            "description": "图片链接",
            "key": "src",
            "type": "String",
            "default": "http://img.lemeitu.com/m00/a0/5c/ffc3c39d289296db3b97effa924d4bb6__w.jpg",
            "inputType": "image"
        },
        {
